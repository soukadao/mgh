{
  "permissions": {
    "allow": [
      "Read(src/**)",
      "Read(package.json)",
      "Read(README.md)",
      "Bash(rg:*)",
      "WebFetch(domain:docs.github.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:vitest.dev)",
      "WebSearch(domain:docs.github.com)",
      "WebSearch(domain:github.com)",
      "WebSearch(domain:vitest.dev)",
      "mcp__github__update_issue"
    ],
    "deny": [
      "Bash(*)",
      "Edit",
      "Write",
      "NotebookEdit",
      "SlashCommand",
      "WebFetch(domain:*)",
      "WebSearch(domain:*)"
    ]
  },
  "disableAllHooks": false,
  "enableAllProjectMcpServers": true
}
